# GitHub API Token (requires read:user scope)
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Users to monitor (JSON array with github username and discord user ID)
USERS=[{"github":"user1","discord":"123456789012345678"},{"github":"user2","discord":"234567890123456789"}]

# Discord Webhook URL
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/xxxxx/yyyyy

# API Key for /check endpoint authentication
API_KEY=your_secret_api_key_here

# Timezone offset from UTC in hours (e.g., 9 for JST, -5 for EST)
TIMEZONE_OFFSET=9
